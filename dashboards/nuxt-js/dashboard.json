          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(apm.service.transaction.duration)  as 'Error duration (Avg)', median(apm.service.transaction.duration) as 'Error duration (Median)', percentile(apm.service.transaction.duration, 90) as 'Error duration' FROM Metric WHERE (entity.guid = 'MzUxMDYxM3xBUE18QVBQTElDQVRJT058NTQ1OTIwODI2') AND (transactionType = 'Web') SINCE '2022-07-24 21:51:00-0700' UNTIL '2022-08-16'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
