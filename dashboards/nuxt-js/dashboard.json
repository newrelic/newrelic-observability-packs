              {
                "accountId": 0,
                "query": "FROM Metric SELECT sum(apm.service.metrics.reported) - sum(apm.service.error.count) AS 'Records' WHERE (entity.guid = 'MzUxMDYxM3xBUE18QVBQTElDQVRJT058NTQ1OTIwODI2') SINCE '2022-07-24 21:51:00-0700'  UNTIL '2022-08-16'"
              }
