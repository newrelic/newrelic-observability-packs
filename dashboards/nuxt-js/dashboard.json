            "column": 9,
            "row": 1,
            "width": 2,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT count(*) AS 'Errors' WHERE (metricName='newrelic.goldenmetrics.apm.application.errorRate') AND (entity.guid = 'MzUxMDYxM3xBUE18QVBQTElDQVRJT058NTQ1OTIwODI2') SINCE '2022-07-24 21:51:00-0700' UNTIL '2022-08-16' "
