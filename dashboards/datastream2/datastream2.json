            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "dataFormatters": [],
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Log SELECT percentage(count (*), WHERE numeric(cacheStatus) = 1) AS 'Cache Hit Ratio' WHERE message='akamai' SINCE 1 hour ago compare with 1 hour ago\n"
