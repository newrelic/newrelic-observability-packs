                "query": "FROM Metric SELECT SUM(`azure.purview.accounts.DataMapCapacityUnits`) AS 'Total Data Map Capacity Units', SUM(`azure.purview.accounts.DataMapStorageSize`) AS 'Total Data Map Storage Size', SUM(`azure.purview.accounts.ScanCancelled`) AS 'Total Scans Cancelled', SUM(`azure.purview.accounts.ScanCompleted`) AS 'Total Scans Completed', SUM(`azure.purview.accounts.ScanFailed`) AS 'Total Scans Failed', AVERAGE(`azure.purview.accounts.ScanTimeTaken`) AS 'Average Scan Time Taken' WHERE collector.name = 'azure-monitor' AND azure.resourceType='microsoft.purview/accounts' WHERE collector.name = 'azure-monitor' and azure.resourceType = 'microsoft.purview/accounts'"
              }
