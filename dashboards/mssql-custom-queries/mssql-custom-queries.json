              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MssqlCustomQuerySample SELECT latest(longRunning_logical_writes_avg) WHERE longRunning_short_text IS NOT NULL FACET label.db_hostname,longRunning_short_text TIMESERIES 1 MINUTE LIMIT MAX"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            },
