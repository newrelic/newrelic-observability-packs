              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM MssqlCustomQuerySample SELECT latest(longRunning_duration_total_ms)/1000 WHERE longRunning_short_text IS NOT NULL FACET label.db_hostname,longRunning_short_text TIMESERIES 1 MINUTE"
                }
              ],
              "yAxisLeft": {
                "zero": false
              }
            },
