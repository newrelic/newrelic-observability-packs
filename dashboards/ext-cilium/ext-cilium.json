            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT (rate(average(cilium_kubernetes_events_total), 1 SECONDS) * 60) FROM Metric WHERE instrumentation.name = 'remote-write' AND (app_kubernetes_io_name LIKE '%cilium%' or app_newrelic_com_name LIKE '%cilium%') FACET tuple(pod, action) LIMIT MAX TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "units": {
              "unit": "REQUESTS_PER_MINUTE"
            }
          }
        },
