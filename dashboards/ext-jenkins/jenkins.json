        {
          "title": "Jenkins Health",
          "layout": {
            "column": 7,
            "row": 3,
            "width": 2,
            "height": 2
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM (from(from Metric SELECT latest(jenkins_health_check_score) as 'result' WHERE instrumentation.name = 'remote-write') select latest(result) facet cases(WHERE result = 1 as 'Ok', WHERE result != 1 as 'Check')) select latest(cases) as '' "
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "thresholds": [
              {
                "alertSeverity": "CRITICAL",
                "value": 0
              },
              {
                "alertSeverity": "WARNING",
                "value": 0.1
              }
            ]
          }
        },
