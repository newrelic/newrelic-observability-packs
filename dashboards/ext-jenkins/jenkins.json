              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT (((latest(vm_memory_total_max) - latest(vm_memory_total_used)) / latest(vm_memory_total_max)) * 100 ) WHERE instrumentation.name = 'remote-write' AND (app_kubernetes_io_name LIKE '%jenkins%' or app_newrelic_com_name LIKE '%jenkins%') FACET instance"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "thresholds": [
              {
                "alertSeverity": "CRITICAL",
                "value": 10
              },
              {
                "alertSeverity": "WARNING",
                "value": 30
              }
            ]
          }
