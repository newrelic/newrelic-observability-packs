            "nrqlQueries": [
              {
                "accountIds": [],
                "query": "FROM Metric SELECT sum(azure.containerregistry.registries.SuccessfulPullCount) AS 'Total Successful Pull Count', sum(azure.containerregistry.registries.SuccessfulPushCount) AS 'Total Successful Push Count', sum(azure.containerregistry.registries.TotalPullCount) AS 'Total Pull Count', sum(azure.containerregistry.registries.TotalPushCount) AS 'Total Push Count', average(azure.containerregistry.registries.StorageUsed) AS 'Avg Storage Used' WHERE collector.name = 'azure-monitor' and azure.resourceType = 'microsoft.containerregistry/registries'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
