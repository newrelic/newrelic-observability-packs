            "nrqlQueries": [
              {
                "accountIds": [],
                "query": "FROM Metric SELECT sum(`azure.Web.serverfarms.BytesReceived`) AS 'Total Bytes Received', sum(`azure.Web.serverfarms.BytesSent`) AS 'Total Bytes Sent', average(`azure.Web.serverfarms.CpuPercentage`) AS 'Avg CPU Percentage', average(`azure.Web.serverfarms.DiskQueueLength`) AS 'Avg Disk Queue Length', average(`azure.Web.serverfarms.MemoryPercentage`) AS 'Avg Memory Percentage' WHERE collector.name = 'azure-monitor' and azure.resourceType = 'microsoft.web/serverfarms'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
