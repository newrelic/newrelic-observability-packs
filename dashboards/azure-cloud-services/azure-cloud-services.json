              {
                "accountIds": [],
                "query": "FROM Metric SELECT AVERAGE(azure.compute.cloudservices.AvailableMemoryBytes) AS 'Average Available Memory Bytes', Average(azure.compute.cloudservices.DiskReadOperations.Sec) AS 'Average Disk Read Operations', AVERAGE(azure.compute.cloudservices.NetworkInTotal) AS 'Average Network In Total', AVERAGE(azure.compute.cloudservices.PercentageCPU) AS 'Average Percentage CPU' WHERE collector.name = 'azure-monitor' and azure.resourceType = 'microsoft.compute/cloudservices'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
