					},
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": false
						},
						"legend": {
							"enabled": true
						},
						"nrqlQueries": [
							{
								"accountIds": [],
								"query": "FROM NextcloudFlexMetrics SELECT latest(ocs.data.activeUsers.last24hours) FACET dateOf(timestamp) TIMESERIES AUTO"
							}
						],
						"platformOptions": {
							"ignoreTimeRange": false
						}
					}
