          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [],
                "query": "FROM Metric SELECT SUM(`gcp.bigquery.storage.stored_bytes`) AS 'Storage Usage (byte)' TIMESERIES AUTO FACET gcp.bigquery.table"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
